{
  "manifest_version": 2,

  "name": "Dota 2 Stats",
  "description": "Shows DotaBuff stats as a popup",
  "version": "1.0.0",

  "default_locale": "en",

  "browser_action": {
  	"default_icon": "icon.png",
  	"default_popup": "popup.html"
  },

  "author": "jkjk",

  "permissions": [
    "http://dotabuff.com/*",
    "http://www.dotabuff.com/*",
    "storage",
    "management"
  ]
}